# syntax=docker/dockerfile:1

FROM openjdk:11

WORKDIR /app
COPY pom.xml README.md ./
COPY .mvn/ .mvn
COPY src/ ./
COPY mvnw ./
COPY mvnw.cmd ./
RUN ./mvnw clean package

CMD java -Dfile.encoding=UTF-8 -classpath ./target/classes:./target/libs/guice-5.0.1.jar:./target/javax.inject-1.jar:/aopalliance/aopalliance/1.0/aopalliance-1.0.jar:/com/google/guava/guava/30.1-jre/guava-30.1-jre.jar:/com/google/guava/failureaccess/1.0.1/failureaccess-1.0.1.jar:/com/google/guava/listenablefuture/9999.0-empty-to-avoid-conflict-with-guava/listenablefuture-9999.0-empty-to-avoid-conflict-with-guava.jar:/com/google/code/findbugs/jsr305/3.0.2/jsr305-3.0.2.jar:/org/checkerframework/checker-qual/3.5.0/checker-qual-3.5.0.jar:/com/google/errorprone/error_prone_annotations/2.3.4/error_prone_annotations-2.3.4.jar:/com/google/j2objc/j2objc-annotations/1.3/j2objc-annotations-1.3.jar:/com/fasterxml/jackson/core/jackson-databind/2.12.2/jackson-databind-2.12.2.jar:/com/fasterxml/jackson/core/jackson-annotations/2.12.2/jackson-annotations-2.12.2.jar:/com/fasterxml/jackson/core/jackson-core/2.12.2/jackson-core-2.12.2.jar:/com/sparkjava/spark-core/2.9.3/spark-core-2.9.3.jar:/org/eclipse/jetty/jetty-server/9.4.31.v20200723/jetty-server-9.4.31.v20200723.jar:/javax/servlet/javax.servlet-api/3.1.0/javax.servlet-api-3.1.0.jar:/org/eclipse/jetty/jetty-http/9.4.31.v20200723/jetty-http-9.4.31.v20200723.jar:/org/eclipse/jetty/jetty-util/9.4.31.v20200723/jetty-util-9.4.31.v20200723.jar:/org/eclipse/jetty/jetty-io/9.4.31.v20200723/jetty-io-9.4.31.v20200723.jar:/org/eclipse/jetty/jetty-webapp/9.4.31.v20200723/jetty-webapp-9.4.31.v20200723.jar:/org/eclipse/jetty/jetty-xml/9.4.31.v20200723/jetty-xml-9.4.31.v20200723.jar:/org/eclipse/jetty/jetty-servlet/9.4.31.v20200723/jetty-servlet-9.4.31.v20200723.jar:/org/eclipse/jetty/jetty-security/9.4.31.v20200723/jetty-security-9.4.31.v20200723.jar:/org/eclipse/jetty/websocket/websocket-server/9.4.31.v20200723/websocket-server-9.4.31.v20200723.jar:/org/eclipse/jetty/websocket/websocket-common/9.4.31.v20200723/websocket-common-9.4.31.v20200723.jar:/org/eclipse/jetty/websocket/websocket-client/9.4.31.v20200723/websocket-client-9.4.31.v20200723